{
  "manifest_version": 2,
  "name": "Я-лидер!",
  "description": "Рабочее расширение для сайта http://ul-lider.ru",
  "version": "2.0.4",

  "permissions": [
    "tabs",
    "http://*/*",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/logo.png"
  },
  "background": {
    "scripts": [
      "lib/jquery.min.js",
      "scripts/background.js"
    ]
  }
}
